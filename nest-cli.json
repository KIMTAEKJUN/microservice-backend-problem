{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nexon-backend-problem/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nexon-backend-problem/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nexon-backend-problem",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "event-service": {
      "type": "application",
      "root": "apps/event-service",
      "entryFile": "main",
      "sourceRoot": "apps/event-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/event-service/tsconfig.app.json"
      }
    },
    "gateway-service": {
      "type": "application",
      "root": "apps/gateway-service",
      "entryFile": "main",
      "sourceRoot": "apps/gateway-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway-service/tsconfig.app.json"
      }
    },
    "nexon-backend-problem": {
      "type": "application",
      "root": "apps/nexon-backend-problem",
      "entryFile": "main",
      "sourceRoot": "apps/nexon-backend-problem/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nexon-backend-problem/tsconfig.app.json"
      }
    }
  }
}